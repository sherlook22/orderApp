{
    "require": {
        "cakephp/chronos": "^2.0",
        "illuminate/database": "^6.14",
        "lcobucci/jwt": "^3.3",
        "php-di/php-di": "^6.0",
        "ramsey/uuid": "^3.9",
        "respect/validation": "^1.1",
        "selective/array-reader": "^0.3.0",
        "selective/config": "^0.1.1",
        "slim/http": "^1.0",
        "slim/psr7": "^1.0",
        "slim/slim": "4.*",
        "vlucas/phpdotenv": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
